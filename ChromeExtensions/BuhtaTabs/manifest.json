{
  "manifest_version": 2,

  "name": "BuhtaTabs",
  "description": "Switch to active tab",

  "version": "1.0",

  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": [ "content.js" ]
    }
  ],

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

   "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]

}
