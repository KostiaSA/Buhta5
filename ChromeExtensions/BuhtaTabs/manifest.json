{
  "manifest_version": 2,

  "name": "BuhtaTabs",
  "description": "Switch to active tab",

  "version": "1.0",

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

   "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]

}
